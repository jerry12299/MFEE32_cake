                <h6>
                    <%= c_class %>
                </h6>
                <p>主題蛋糕 / <%= c_class %>
                </p>
            </div>
            <div class="PMCake_show_commodity">
                <% data.forEach((item,index)=>{%>
                    <div class="commodity_item">
                        <div>
                            <a href="/C01_2/<%= item.c_id %>">
                                <img src="../Source/IMG/<%= item.img1 %>" alt="<%= item.c_name %>">
                            </a>
                        </div>
                        <div>
                            <p>
                                <%= item.c_name %>
                            </p>
                            <p>NT.<%= item.price %>
